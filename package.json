{
  "name": "netlify-discord-plugin",
  "version": "1.0.0",
  "description": "Plugin Discord for Netlify Builds",
  "author": "Dev0ph3r",
  "private": true,
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "https://github.com/Dev0ph3r/netlify-discord-plugin",
  "keywords": [
    "netlify-plugin",
    "netlify",
    "discord"
  ],
  "scripts": {
    "format": "prettier --write \"./lib/*.js\""
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0"
  },
  "packageManager": "yarn@4.3.1",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  }
}
